/**
 * A function that convert a value.
 */
type Reviver = (value: any) => any | undefined;
/**
 * Parse a `json` string to a value.
 * @param value The `json` value to parse.
 * @param reviver A function to convert a value.
 * @returns The parsed value.
 */
declare function parse(value: string, reviver?: Reviver): unknown;
/**
 * Takes a stream and parse each value until it resolves.
 * @param stream The stream to parse.
 * @param reviver A function to convert a value.
 * @returns A promise that resolve to the parsed value.
 */
declare function parseFromStream(stream: ReadableStream<string>, reviver?: Reviver): Promise<unknown>;

export { type Reviver, parse, parseFromStream };
